<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="167.0" prefWidth="455.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/15.0.1" fx:controller="portfolio.views.NoDataView">
   <children>
      <Label fx:id="infoText" layoutX="145.0" layoutY="14.0" prefHeight="106.0" prefWidth="287.0" text="Es konnten keine Daten geladen werden. Weite Informationen im Wiki." wrapText="true">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <ImageView fx:id="img" fitHeight="72.0" fitWidth="78.0" layoutX="37.0" layoutY="34.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../icons/Help.png" />
         </image>
      </ImageView>
      <Button fx:id="btnClose" layoutX="362.0" layoutY="106.0" mnemonicParsing="false" onMouseClicked="#Close" prefHeight="40.0" prefWidth="55.0">
      <graphic>
         <ImageView fx:id="img" fitHeight="22.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../../icons/exit.png" />
            </image>
         </ImageView>
      </graphic>
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor>
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
      </Button>
      <Button fx:id="btnOpenWiki" layoutX="256.0" layoutY="107.0" mnemonicParsing="false" onAction="#btnWikiclicked" prefHeight="40.0" prefWidth="93.0" text="Wiki FAQ">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <font>
            <Font size="14.0" />
         </font>
      </Button>

   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</AnchorPane>
